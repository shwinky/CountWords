    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.3",
    "jest-junit": "^11.0.1",
    "lodash": "^4.17.19",
    "morgan": "~1.9.1",
    "n-readlines": "^1.0.0",
    "pug": "3.0.1",
    "readline": "^1.3.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
